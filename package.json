{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "A meal booking platform",
  "main": "index.js",
  "scripts": {
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:all": "npx sequelize-cli db:seed:all",
    "start": "node build/index.js",
    "build": "babel api --out-dir build",
    "dev-start": "nodemon --exec babel-node index.js",
    "test": "nyc --require @babel/polyfill --require @babel/register mocha test/ --timeout 10000 --exit",
    "cover": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coveralls": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prince-curie/book-a-meal.git"
  },
  "author": "Prince-Curie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prince-curie/book-a-meal/issues"
  },
  "homepage": "https://github.com/prince-curie/book-a-meal#readme",
  "engines": {
    "node": "12.18.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "init": "^0.1.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-datetime": "^1.5.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^8.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sequelize-cli": "^5.5.0"
  }
}
